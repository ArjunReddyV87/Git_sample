{
	"name": "Add_2Num",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedcolumn"
				},
				{
					"name": "inDataset"
				}
			],
			"scriptLines": [
				"parameters{",
				"     p_col1 as string,",
				"     p_col2 as string",
				"}",
				"input(order: 0,",
				"     allowSchemaDrift: true) ~> inDataset",
				"inDataset derive(sum_value = toInteger(byName($p_col1)) + toInteger(byName($p_col2))) ~> derivedcolumn"
			]
		}
	}
}