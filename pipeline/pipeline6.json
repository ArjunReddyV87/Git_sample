					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "SalesID",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "SalesID",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "CustomerID",
